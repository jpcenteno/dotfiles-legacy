{
   "title":"Launch different applications",
   "rules":[
      {
         "description":"⌘ +⇧ +↵  to open new Terminal.app window.",
         "manipulators":[
            {
               "from":{
                  "key_code":"return_or_enter",
                  "modifiers":{
                     "mandatory":[
                        "left_command",
                        "left_shift"
                     ]
                  }
               },
               "to":[
                  {
                     "shell_command":"osascript ~/.config/karabiner/scripts/OpenTerminalWindow.scpt"
                  }
               ],
               "type":"basic"
            }
         ]
      }
   ]
}
